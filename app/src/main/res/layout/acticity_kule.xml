<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipChildren="false"
    android:clipToPadding="false">

    <com.iptv.hq.ui.views.dynamicbg.DynamicViewBG
        android:id="@+id/kb_bg"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <ImageView
        android:id="@+id/logo"
        android:layout_width="@dimen/px156"
        android:layout_height="@dimen/px60"
        android:layout_marginLeft="@dimen/px80"
        android:layout_marginTop="@dimen/px50"
        android:scaleType="fitXY"
        android:src="@mipmap/logo_2" />

    <RelativeLayout
        android:id="@+id/r_main_lay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/px130">

        <com.iptv.hq.ui.views.BorderImageView
            android:id="@+id/item_1"
            android:layout_width="@dimen/px217"
            android:layout_height="@dimen/px261"
            android:layout_margin="@dimen/px7"
            android:clickable="true"
            android:focusable="true"
            android:focusableInTouchMode="true" />

        <com.iptv.hq.ui.views.BorderImageView
            android:id="@+id/item_2"
            android:layout_width="@dimen/px174"
            android:layout_height="@dimen/px77"
            android:layout_margin="@dimen/px7"
            android:layout_toRightOf="@+id/item_1"
            android:clickable="true"
            android:focusable="true"
            android:focusableInTouchMode="true" />

        <com.iptv.hq.ui.views.BorderImageView
            android:id="@+id/item_3"
            android:layout_width="@dimen/px174"
            android:layout_height="@dimen/px77"
            android:layout_below="@+id/item_2"
            android:layout_margin="@dimen/px7"
            android:layout_toRightOf="@+id/item_1"
            android:clickable="true"
            android:focusable="true"
            android:focusableInTouchMode="true" />

        <com.iptv.hq.ui.views.BorderImageView
            android:id="@+id/item_4"
            android:layout_width="@dimen/px174"
            android:layout_height="@dimen/px77"
            android:layout_below="@+id/item_3"
            android:layout_margin="@dimen/px7"
            android:layout_toRightOf="@+id/item_1"
            android:clickable="true"
            android:focusable="true"
            android:focusableInTouchMode="true" />

        <com.iptv.hq.ui.views.BorderImageView
            android:id="@+id/item_5"
            android:layout_width="@dimen/px406"
            android:layout_height="@dimen/px228"
            android:layout_below="@+id/item_4"
            android:layout_margin="@dimen/px7"
            android:clickable="true"
            android:focusable="true"
            android:focusableInTouchMode="true" />

        <com.iptv.hq.ui.views.BorderImageView
            android:id="@+id/item_6"
            android:layout_width="@dimen/px449"
            android:layout_height="@dimen/px261"
            android:layout_margin="@dimen/px7"
            android:layout_toRightOf="@+id/item_4"
            android:clickable="true"
            android:focusable="true"
            android:focusableInTouchMode="true" />

        <com.iptv.hq.ui.views.BorderImageView
            android:id="@+id/item_7"
            android:layout_width="@dimen/px217"
            android:layout_height="@dimen/px228"
            android:layout_below="@+id/item_6"
            android:layout_margin="@dimen/px7"
            android:layout_toRightOf="@+id/item_4"
            android:clickable="true"
            android:focusable="true"
            android:focusableInTouchMode="true" />

        <com.iptv.hq.ui.views.BorderImageView
            android:id="@+id/item_8"
            android:layout_width="@dimen/px217"
            android:layout_height="@dimen/px228"
            android:layout_below="@+id/item_6"
            android:layout_margin="@dimen/px7"
            android:layout_toRightOf="@+id/item_7"
            android:clickable="true"
            android:focusable="true"
            android:focusableInTouchMode="true" />

        <com.iptv.hq.ui.views.BorderImageView
            android:id="@+id/item_9"
            android:layout_width="@dimen/px265"
            android:layout_height="@dimen/px503"
            android:layout_margin="@dimen/px7"
            android:layout_toRightOf="@+id/item_8"
            android:clickable="true"
            android:focusable="true"
            android:focusableInTouchMode="true" />
    </RelativeLayout>
</RelativeLayout>